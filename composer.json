{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "box/spout": "^2.7",
        "doctrine/dbal": "^2.6",
        "doctrine/lexer": "^1.0.1",
        "eluceo/ical": "^0.11.3",
        "fideloper/proxy": "^4.0",
        "hashids/hashids": "^2.0",
        "kitetail/zttp": "^0.6.0",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.3",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "1.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "wnx/laravel-stats": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
